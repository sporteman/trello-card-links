   "manifest_version": 2,
   "name": "Parent Board/Child Management for Trello",
   "options_page": "options.html",
   "permissions": [ "tabs", "storage", "https://trello.com/", "https://api.airtable.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
