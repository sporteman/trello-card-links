{
   "author": "Deivison Sporteman",
   "content_scripts": [ {
      "css": [ "css/style.css" ],
      "js": [ "js/vendor/jquery.min.js", "js/vendor/jquery-ui.min.js", "js/pluggins/pluggin.js", "js/config.js", "js/api.js", "js/init.js" ],
      "matches": [ "https://trello.com/*" ]
